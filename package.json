{
  "name": "roads-upon",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build": "eleventy && npm run sass",
    "serve": "concurrently 'eleventy --serve' 'npm run sass:watch'",
    "sass": "sass styles/:_site/styles/ --no-source-map --style=compressed --update",
    "sass:watch": "npm run sass && npm run sass -- -w",
    "clean": "rm -rf _site"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpitbatra123/roads-upon.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arpitbatra123/roads-upon/issues"
  },
  "homepage": "https://github.com/arpitbatra123/roads-upon#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "concurrently": "^6.2.0",
    "sass": "^1.35.1"
  }
}
